{
    "name": "web",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/web/src",
    "targets": {
        "dev": {
            "command": "vite",
            "options": {
                "cwd": "apps/web"
            }
        },
        "build": {
            "command": "vite build",
            "options": {
                "cwd": "apps/web"
            },
            "outputs": ["{projectRoot}/dist"]
        },
        "preview": {
            "command": "vite preview",
            "options": {
                "cwd": "apps/web"
            }
        },
        "test": {
            "command": "vitest run",
            "options": {
                "cwd": "apps/web"
            }
        },
        "lint": {
            "command": "eslint .",
            "options": {
                "cwd": "apps/web"
            }
        }
    }
}
